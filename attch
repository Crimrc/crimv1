                return L_460_(unpack(L_487_))
            end
            return L_460_(...)
        end
    )
        game.Players.LocalPlayer.Chatted:Connect(
        function(L_488_arg0)
            if L_488_arg0 == "!check" then
                 notify("ZZZ", "Check Prediction F9")
                print(_G.PRED)
